<Window x:Class="TestovoeNabiullinVladislav.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestovoeNabiullinVladislav.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <Frame Content="{Binding MainFrame.Content}" Grid.Column="1" Margin="7"/>
        <!--<Label Content="Тестовое Набиуллин В.А" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="1" FontSize="40"/>-->
        <StackPanel>
            <Button Height="30" Margin="5" Content="Администратор" Command="{Binding OpenAdminPanelCommand}"/>
        </StackPanel>
    </Grid>
</Window>
